<div class="container">
  
  @if (User.Identity.IsAuthenticated)
  {
    <img src="https://images.unsplash.com/photo-1460467820054-c87ab43e9b59?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1067&q=80" alt="come on in sign" border="0">
    <p>Hi @User.Identity.Name!</p>
    @using (Html.BeginForm("LogOff", "Account"))
    {
        <div id="table">
        <input type="submit" value="Log out" />
        </div>
    }
  }
  else
  {
  <img src="https://images.unsplash.com/photo-1655648340915-019ee5a3fb2a?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1470&q=80" alt="sad dog" border="0">
  <hr />
    <table id="table">
      <tr>
        <td>@Html.ActionLink("Register", "Register")</td>
        <td>@Html.ActionLink("Log in", "Login")</td>
      </tr>
    </table>
  }
  <p>@Html.ActionLink("Home", "Index", "Home")</p>
</div>